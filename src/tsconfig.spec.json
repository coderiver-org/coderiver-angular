{
  "extends": "../tsconfig.json",
  "baseUrl": "./",
  "compilerOptions": {
    "outDir": "../out-tsc/spec",
    "module": "commonjs",
    "types": ["jest"],
    "target": "es2015",
    "lib": [
      "es5",
      "es6",
      "dom",
      "es2018",
      "es2017",
      "es2015.core",
      "es2015.collection",
      "es2015.generator",
      "es2015.iterable",
      "es2015.promise",
      "es2015.proxy",
      "es2015.reflect",
      "es2015.symbol",
      "es2015.symbol.wellknown",
      "esnext.asynciterable"
    ]
  },
  "files": [
    "test.ts",
    "polyfills.ts"
  ],
  "include": [
    "**/*.spec.ts",
    "**/*.d.ts",
    "**/*.ts"
  ],
  "exclude": [
    "testing/common.spec.ts"
  ]
}
